# Production Environment Variables Template
# Copy this file to .env.production and fill with actual values

# Database Configuration
DATABASE_URL=postgresql://username:password@postgres:5432/database_name

# N8N Webhook URLs
N8N_WEBHOOK_URL=https://webhookn8n.odtravel.com.br/webhook/your-webhook-id
N8N_ALERT_WEBHOOK_URL=https://n8n.odtravel.com.br/webhook-test/your-alert-webhook-id

# API Keys
GEMINI_API_KEY=your-gemini-api-key

# CORS Configuration
CORS_ORIGIN=https://agendamento.odmax.com.br,https://chatwoot.yourdomain.com

# Additional Security Settings
JWT_SECRET=your-jwt-secret-key
ENCRYPTION_KEY=your-encryption-key